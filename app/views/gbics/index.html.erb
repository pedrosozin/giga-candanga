<p id="notice"><%= notice %></p>

<h1>Gbics</h1>

<table>
  <thead>
    <tr>
      <th>Porta slot</th>
      <th>Serie</th>
      <th>Patrimonio</th>
      <th>Tipo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @gbics.each do |gbic| %>
      <tr>

        <td><% if gbic.porta_slot %><%= gbic.porta_slot.porta %> <% end %></td>
        <td><%= gbic.serie %></td>
        <td><%= gbic.patrimonio %></td>
        <td><%= gbic.tipo %></td>
        <td><%= link_to 'Show', gbic %></td>
        <td><%= link_to 'Edit', edit_gbic_path(gbic) %></td>
        <td><%= link_to 'Destroy', gbic, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Gbic', new_gbic_path %>
