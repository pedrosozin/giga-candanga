<p id="notice"><%= notice %></p>

<h1>Jumpers</h1>

<table>
  <thead>
    <tr>
      <th>Portadgo</th>
      <th>Portadgo2</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @jumpers.each do |jumper| %>
      <tr>
        <td><%= jumper.portadgo.cod %></td>
        <td><%= jumper.portadgo2.cod %></td>
        <td><%= link_to 'Show', jumper %></td>
        <td><%= link_to 'Edit', edit_jumper_path(jumper) %></td>
        <td><%= link_to 'Destroy', jumper, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Jumper', new_jumper_path %>
