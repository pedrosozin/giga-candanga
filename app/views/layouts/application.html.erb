<!DOCTYPE html>
<html>
  <head>
    <title>GigaCandanga</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all'%>
    <%= javascript_include_tag 'application'%>

    <% if current_usuario %>
      <%= link_to "Deslogar", destroy_usuario_session_path, method: :delete %>
    <% end %>
  </head>

  <body>
    <div class="page-wrapper">
      <%= render "layouts/navbar"%>
      <%= render "layouts/sidebar"%>
      <%= yield %>
    </div>
  </body>

</html>
