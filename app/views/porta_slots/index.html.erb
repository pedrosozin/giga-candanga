<p id="notice"><%= notice %></p>

<h1>Porta Slots</h1>

<table>
  <thead>
    <tr>
      <th>Slot</th>
      <th>Porta</th>
      <th>Tipo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @porta_slots.each do |porta_slot| %>
      <tr>
        <td><%= porta_slot.slot.serie %></td>
        <td><%= porta_slot.porta %></td>
        <td><%= porta_slot.tipo %></td>
        <td><%= link_to 'Show', porta_slot %></td>
        <td><%= link_to 'Edit', edit_porta_slot_path(porta_slot) %></td>
        <td><%= link_to 'Destroy', porta_slot, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Porta Slot', new_porta_slot_path %>
