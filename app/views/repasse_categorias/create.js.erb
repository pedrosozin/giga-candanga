<%if @repasse_categoria.persisted? %>
    $('#categorias-ativas-listing').fadeOut(400, function(){
      $('.dialog-wrapper').fadeOut(400);
      $(this).remove();
      let myTable = $('<%= j render(partial: "tables", locals: {repasse_categorias: @repasse_categorias_ativas, identifier: "categorias-ativas-listing", icone:"ion-close-circled" })%>');
      myTable.hide();
      myTable.insertAfter($('.pre-ativas')).fadeIn(400);
    });

<%else%>
    alert("Nao foi poss√≠vel desativar registro!");
<%end%>

