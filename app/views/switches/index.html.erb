<p id="notice"><%= notice %></p>

<h1>Switches</h1>

<table>
  <thead>
    <tr>
      <th>Site</th>
      <th>Serie</th>
      <th>Fabricante</th>
      <th>Qtd slots</th>
      <th>Patrimonio</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @switches.each do |switch| %>
      <tr>
        <td><%= switch.site.nome %></td>
        <td><%= switch.serie %></td>
        <td><%= switch.fabricante %></td>
        <td><%= switch.qtd_slots %></td>
        <td><%= switch.patrimonio %></td>
        <td><%= link_to 'Show', switch %></td>
        <td><%= link_to 'Edit', edit_switch_path(switch) %></td>
        <td><%= link_to 'Destroy', switch, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Switch', new_switch_path %>
